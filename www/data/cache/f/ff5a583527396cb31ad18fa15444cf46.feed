&lt;a target=&quot;_blank&quot;  href=&quot;http://wiki.r-project.org/rwiki/rhelp.php?id=plot&quot;&gt;&lt;span class=&quot;kw3&quot;&gt;plot&lt;/span&gt;&lt;/a&gt;&lt;span class=&quot;br0&quot;&gt;&amp;#40;&lt;/span&gt;&lt;span class=&quot;re5&quot;&gt;xx&lt;/span&gt;,y&lt;span class=&quot;re5&quot;&gt;y&lt;/span&gt;,type=&lt;span class=&quot;st0&quot;&gt;&quot;n&quot;&lt;/span&gt;,xlab=&lt;span class=&quot;st0&quot;&gt;&quot;x&quot;&lt;/span&gt;,ylab=&lt;span class=&quot;st0&quot;&gt;&quot;y&quot;&lt;/span&gt;&lt;span class=&quot;br0&quot;&gt;&amp;#41;&lt;/span&gt;
