&lt;pre class=&quot;code r&quot;&gt;&lt;a target=&quot;_blank&quot;  href=&quot;http://wiki.r-project.org/rwiki/rhelp.php?id=vi&quot;&gt;&lt;span class=&quot;kw4&quot;&gt;vi&lt;/span&gt;&lt;/a&gt;.grid.illusion&lt;span class=&quot;br0&quot;&gt;&amp;#40;&lt;/span&gt;&lt;span class=&quot;re5&quot;&gt;type&lt;/span&gt; = &lt;span class=&quot;st0&quot;&gt;&quot;h&quot;&lt;/span&gt;, &lt;span class=&quot;re5&quot;&gt;lwd&lt;/span&gt; = &lt;span class=&quot;nu0&quot;&gt;22&lt;/span&gt;, &lt;span class=&quot;re5&quot;&gt;nrow&lt;/span&gt; = &lt;span class=&quot;nu0&quot;&gt;5&lt;/span&gt;, &lt;span class=&quot;re5&quot;&gt;ncol&lt;/span&gt; = &lt;span class=&quot;nu0&quot;&gt;5&lt;/span&gt;, 
    &lt;span class=&quot;re5&quot;&gt;col&lt;/span&gt; = &lt;span class=&quot;st0&quot;&gt;&quot;white&quot;&lt;/span&gt;&lt;span class=&quot;br0&quot;&gt;&amp;#41;&lt;/span&gt;&lt;/pre&gt;
