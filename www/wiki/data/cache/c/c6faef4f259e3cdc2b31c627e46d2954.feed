=&quot;btnPausesrswr&quot; title=&quot;Pause&quot; onclick=&quot;pauseAni('srswr')&quot; value=&quot;  O  &quot; /&gt; &lt;input name=&quot;btnFastsrswr&quot; type=&quot;submit&quot; disabled=&quot;disabled&quot; class=&quot;anibutton&quot; id=&quot;btnFastsrswr&quot; title=&quot;Speed up&quot; onclick=&quot;fastAni('srswr',1)&quot; value=&quot;  +  &quot; /&gt; &lt;input name=&quot;btnSlowsrswr&quot; type=&quot;submit&quot; disabled=&quot;disabled&quot; class=&quot;anibutton&quot; id=&quot;btnSlowsrswr&quot; title=&quot;Slow down&quot; onclick=&quot;fastAni('srswr',-1)&quot; value=&quot;  -  &quot; /&gt; &lt;input name=&quot;btnPrevsrswr&quot; type=&quot;submit&quot; disabled=&quot;disabled&quot; class=&quot;anibutton&quot; id=&quot;btnPrevsrswr&quot; title=&quot;Previous frame&quot; onclick=&quot;prevAni('srswr',1)&quot; value=&quot;  &amp;lt;&amp;lt;  &quot; /&gt; &lt;input name=&quot;btnNextsrswr&quot; type=&quot;submit&quot; disabled=&quot;disabled&quot; class=&quot;anibutton&quot; id=&quot;btnNextsrswr&quot; title=&quot;Next frame&quot; onclick=&quot;prevAni('srswr',-1)&quot; value=&quot;  &amp;gt;&amp;gt;  &quot; /&gt; &lt;input name=&quot;btnFirstsrswr&quot; type=&quot;submit&quot; disabled=&quot;disabled&quot; class=&quot;anibutton&quot; id=&quot;btnFirstsrswr&quot; title=&quot;First frame&quot; onclick=&quot;firstAni('srswr',true)&quot; value=&quot;  |&amp;lt;  &quot; /&gt; &lt;input name=&quot;btnLastsrswr&quot; type=&quot;submit&quot; disabled=&quot;disabled&quot; class=&quot;anibutton&quot; id=&quot;btnLastsrswr&quot; title=&quot;Last frame&quot; onclick=&quot;firstAni('srswr',false)&quot; value=&quot;  &amp;gt;|  &quot; /&gt; &lt;input name=&quot;btnMoresrswr&quot; type=&quot;submit&quot; disabled=&quot;disabled&quot; class=&quot;anibutton&quot; id=&quot;btnMoresrswr&quot; title=&quot;More controls&quot; onclick=&quot;btnMore('srswr')&quot; value=&quot;  &amp;lt;&amp;gt;  &quot; /&gt;&lt;/div&gt;&lt;div id=&quot;moreParsrswr&quot; class=&quot;morepar&quot;&gt;&lt;label title=&quot;Which frame to go?&quot; onmouseover=&quot;this.style.backgroundColor='#8cacbb'&quot; onmouseout=&quot;this.style.backgroundColor='#ffffff'&quot;&gt;View frame &lt;input name=&quot;txtFramesrswr&quot; type=&quot;text&quot; id=&quot;txtFramesrswr&quot; onblur=&quot;txtFrame(this.value, 'srswr')&quot; value=&quot;1&quot; size=&quot;4&quot; class=&quot;text&quot; /&gt;/30&lt;/label&gt; &lt;label title=&quot;Loop or not?&quot; onmouseover=&quot;this.style.backgroundColor='#8cacbb'&quot; onmouseout=&quot;this.style.backgroundColor='#ffffff'&quot;&gt;&lt;input name=&quot;checkLoopsrswr&quot; type=&quot;checkbox&quot; id=&quot;checkLoopsrswr&quot; checked=&quot;checked&quot;/&gt;Loop&lt;/label&gt; &lt;label title=&quot;Time interval for the animation (in sec)&quot; onmouseover=&quot;this.style.backgroundColor='#8cacbb'&quot; onmouseout=&quot;this.style.backgroundColor='#ffffff'&quot;&gt;Time Interval: &lt;input name=&quot;txtIntervalsrswr&quot; type=&quot;text&quot; id=&quot;txtIntervalsrswr&quot; onblur=&quot;txtInterval(this)&quot; value=&quot;1&quot; size=&quot;4&quot; class=&quot;text&quot; /&gt;&lt;/label&gt; &lt;label title=&quot;Increment in time interval&quot; onmouseover=&quot;this.style.backgroundColor='#8cacbb'&quot; onmouseout=&quot;this.style.backgroundColor='#ffffff'&quot;&gt;Step: &lt;input name=&quot;txtStepsrswr&quot; type=&quot;text&quot; id=&quot;txtStepsrswr&quot; onblur=&quot;txtInterval(this)&quot; value=&quot;0.1&quot; size=&quot;4&quot; class=&quot;text&quot; /&gt;&lt;/label&gt;&lt;/div&gt;&lt;hr /&gt;&lt;div class=&quot;description&quot;&gt;
Each member of the population has an equal and known chance of being selected.
&lt;/div&gt;&lt;/fieldset&gt;&lt;/div&gt;
&lt;/p&gt;

&lt;/div&gt;
