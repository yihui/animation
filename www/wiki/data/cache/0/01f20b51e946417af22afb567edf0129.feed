        <item>
            <title>compstat:gradient_descent_algorithm</title>
            <link>http://animation.yihui.name/compstat:gradient_descent_algorithm</link>
            <description>


&lt;h1&gt;&lt;a name=&quot;the_gradient_descent_algorithm&quot; id=&quot;the_gradient_descent_algorithm&quot;&gt;The Gradient Descent Algorithm&lt;/a&gt;&lt;/h1&gt;
&lt;div class=&quot;level1&quot;&gt;

&lt;p&gt;

Follow the gradient to the optimum. &lt;img src=&quot;http://animation.yihui.name/lib/images/smileys/fixme.gif&quot; class=&quot;middle&quot; alt=&quot;FIXME&quot; /&gt;
&lt;/p&gt;

&lt;/div&gt;
&lt;!-- SECTION &quot;The Gradient Descent Algorithm&quot; [1-89] --&gt;
&lt;h2&gt;&lt;a name=&quot;animation&quot; id=&quot;animation&quot;&gt;Animation&lt;/a&gt;&lt;/h2&gt;
&lt;div class=&quot;level2&quot;&gt;

&lt;p&gt;

&lt;div align=&quot;center&quot; class=&quot;anidemo&quot; title=&quot;Demonstration of the Gradient Descent Algorithm&quot;&gt;&lt;fieldset&gt;&lt;legend align=&quot;center&quot;&gt;Demonstration of the Gradient Descent Algorithm&lt;/legend&gt;&lt;div id=&quot;loadinggda&quot; class=&quot;loading&quot;&gt;loading animation frames... 0%&lt;/div&gt;&lt;div id=&quot;divPreloadgda&quot; class=&quot;divPreload&quot;&gt;&lt;script language=&quot;JavaScript&quot; type=&quot;text/javascript&quot;&gt;	var sourcegda = &quot;http://s288.photobucket.com/albums/ll181/xieyihui/gradient_descent_algorithm/&quot;;	var imgtypegda = &quot;png&quot;;	var tmpgda = 0;	var nmaxgda=36; 	var ngda=1;	var tgda;	for(i = 1; i &lt;= nmaxgda; i++){		document.write(&quot;&lt;div class=\&quot;divFrame\&quot; id=\&quot;divPreload&quot; + &quot;gda&quot; + i + &quot;\&quot;&gt;&lt;img src=\&quot;&quot; + sourcegda + i + &quot;.&quot; + imgtypegda + &quot;\&quot;&quot; + &quot; id = \&quot;img&quot; + &quot;gda&quot; + i + &quot;\&quot; onload = \&quot;loading(\'gda\')\&quot; onerror = \&quot;loading(\'gda\')\&quot; alt=\&quot;Demonstration of the Gradient Descent Algorithm\&quot; title=\&quot;Demonstration of the Gradient Descent Algorithm\&quot; /&gt;&lt;/div&gt;&quot;);}&lt;/script&gt;&lt;/div&gt;&lt;div class=&quot;btncontrol&quot;&gt;&lt;input name=&quot;nmaxgda&quot; type=&quot;hidden&quot; id=&quot;nmaxgda&quot; value=&quot;36&quot; /&gt;&lt;input name=&quot;heightgda&quot; type=&quot;hidden&quot; id=&quot;heightgda&quot; value=&quot;500&quot; /&gt;&lt;input name=&quot;btnPlaygda&quot; type=&quot;submit&quot; disabled=&quot;disabled&quot; class=&quot;anibutton&quot; id=&quot;btnPlaygda&quot; title=&quot;Play&quot; onclick=&quot;playAni('gda')&quot; value=&quot;  &amp;gt;  &quot; /&gt; &lt;input name=&quot;btnPausegda&quot; type=&quot;submit&quot; disabled=&quot;disabled&quot; class=&quot;anibutton&quot; id=&quot;btnPausegda&quot; title=&quot;Pause&quot; onclick=&quot;pauseAni('gda')&quot; value=&quot;  O  &quot; /&gt; &lt;input name=&quot;btnFastgda&quot; type=&quot;submit&quot; disabled=&quot;disabled&quot; class=&quot;anibutton&quot; id=&quot;btnFastgda&quot; title=&quot;Speed up&quot; onclick=&quot;fastAni('gda',1)&quot; value=&quot;  +  &quot; /&gt; &lt;input name=&quot;btnSlowgda&quot; type=&quot;submit&quot; disabled=&quot;disabled&quot; class=&quot;anibutton&quot; id=&quot;btnSlowgda&quot; title=&quot;Slow down&quot; onclick=&quot;fastAni('gda',-1)&quot; value=&quot;  -  &quot; /&gt; &lt;input name=&quot;btnPrevgda&quot; type=&quot;submit&quot; disabled=&quot;disabled&quot; class=&quot;anibutton&quot; id=&quot;btnPrevgda&quot; title=&quot;Previous frame&quot; onclick=&quot;prevAni('gda',1)&quot; value=&quot;  &amp;lt;&amp;lt;  &quot; /&gt; &lt;input name=&quot;btnNextgda&quot; type=&quot;submit&quot; disabled=&quot;disabled&quot; class=&quot;anibutton&quot; id=&quot;btnNextgda&quot; title=&quot;Next frame&quot; onclick=&quot;prevAni('gda',-1)&quot; value=&quot;  &amp;gt;&amp;gt;  &quot; /&gt; &lt;input name=&quot;btnFirstgda&quot; type=&quot;submit&quot; disabled=&quot;disabled&quot; class=&quot;anibutton&quot; id=&quot;btnFirstgda&quot; title=&quot;First frame&quot; onclick=&quot;firstAni('gda',true)&quot; value=&quot;  |&amp;lt;  &quot; /&gt; &lt;input name=&quot;btnLastgda&quot; type=&quot;submit&quot; disabled=&quot;disabled&quot; class=&quot;anibutton&quot; id=&quot;btnLastgda&quot; title=&quot;Last frame&quot; onclick=&quot;firstAni('gda',false)&quot; value=&quot;  &amp;gt;|  &quot; /&gt; &lt;input name=&quot;btnMoregda&quot; type=&quot;submit&quot; disabled=&quot;disabled&quot; class=&quot;anibutton&quot; id=&quot;btnMoregda&quot; title=&quot;More controls&quot; onclick=&quot;btnMore('gda')&quot; value=&quot;  &amp;lt;&amp;gt;  &quot; /&gt;&lt;/div&gt;&lt;div id=&quot;morePargda&quot; class=&quot;morepar&quot;&gt;&lt;label title=&quot;Which frame to go?&quot; onmouseover=&quot;this.style.backgroundColor='#8cacbb'&quot; onmouseout=&quot;this.style.backgroundColor='#ffffff'&quot;&gt;View frame &lt;input name=&quot;txtFramegda&quot; type=&quot;text&quot; id=&quot;txtFramegda&quot; onblur=&quot;txtFrame(this.value, 'gda')&quot; value=&quot;1&quot; size=&quot;4&quot; class=&quot;text&quot; /&gt;/36&lt;/label&gt; &lt;label title=&quot;Loop or not?&quot; onmouseover=&quot;this.style.backgroundColor='#8cacbb'&quot; onmouseout=&quot;this.style.backgroundColor='#ffffff'&quot;&gt;&lt;input name=&quot;checkLoopgda&quot; type=&quot;checkbox&quot; id=&quot;checkLoopgda&quot; checked=&quot;checked&quot;/&gt;Loop&lt;/label&gt; &lt;label title=&quot;Time interval for the animation (in sec)&quot; onmouseover=&quot;this.style.backgroundColor='#8cacbb'&quot; onmouseout=&quot;this.style.backgroundColor='#ffffff'&quot;&gt;Time Interval: &lt;input name=&quot;txtIntervalgda&quot; type=&quot;text&quot; id=&quot;txtIntervalgda&quot; onblur=&quot;txtInterval(this)&quot; value=&quot;0.3&quot; size=&quot;4&quot; class=&quot;text&quot; /&gt;&lt;/label&gt; &lt;label title=&quot;Increment in time interval&quot; onmouseover=&quot;this.style.backgroundColor='#8cacbb'&quot; onmouseout=&quot;this.style.backgroundColor='#ffffff'&quot;&gt;Step: &lt;input name=&quot;txtStepgda&quot; type=&quot;text&quot; id=&quot;txtStepgda&quot; onblur=&quot;txtInterval(this)&quot; value=&quot;0.1&quot; size=&quot;4&quot; class=&quot;text&quot; /&gt;&lt;/label&gt;&lt;/div&gt;&lt;hr /&gt;&lt;div class=&quot;description&quot;&gt;
The arrows will take you to the optimum step by step.
&lt;/div&gt;&lt;/fieldset&gt;&lt;/div&gt;
&lt;/p&gt;

&lt;/div&gt;
&lt;!-- SECTION &quot;Animation&quot; [90-325] --&gt;
&lt;h2&gt;&lt;a name=&quot;r_code&quot; id=&quot;r_code&quot;&gt;R code&lt;/a&gt;&lt;/h2&gt;
&lt;div class=&quot;level2&quot;&gt;
&lt;pre class=&quot;code r&quot;&gt;&lt;span class=&quot;re5&quot;&gt;oopt&lt;/span&gt; = ani.options&lt;span class=&quot;br0&quot;&gt;&amp;#40;&lt;/span&gt;&lt;span class=&quot;re4&quot;&gt;&lt;span class=&quot;re5&quot;&gt;ani&lt;/span&gt;&lt;/span&gt;&lt;span class=&quot;re5&quot;&gt;.height&lt;/span&gt; = &lt;span class=&quot;nu0&quot;&gt;500&lt;/span&gt;, &lt;span class=&quot;re4&quot;&gt;&lt;span class=&quot;re5&quot;&gt;ani&lt;/span&gt;&lt;/span&gt;&lt;span class=&quot;re5&quot;&gt;.width&lt;/span&gt; = &lt;span class=&quot;nu0&quot;&gt;500&lt;/span&gt;, &lt;span class=&quot;re5&quot;&gt;outdir&lt;/span&gt; = &lt;a target=&quot;_blank&quot;  href=&quot;http://wiki.r-project.org/rwiki/rhelp.php?id=getwd&quot;&gt;&lt;span class=&quot;kw4&quot;&gt;getwd&lt;/span&gt;&lt;/a&gt;&lt;span class=&quot;br0&quot;&gt;&amp;#40;&lt;/span&gt;&lt;span class=&quot;br0&quot;&gt;&amp;#41;&lt;/span&gt;, &lt;span class=&quot;re5&quot;&gt;interval&lt;/span&gt; = &lt;span class=&quot;nu0&quot;&gt;0&lt;/span&gt;&lt;span class=&quot;re5&quot;&gt;.&lt;span class=&quot;nu0&quot;&gt;3&lt;/span&gt;&lt;/span&gt;,
